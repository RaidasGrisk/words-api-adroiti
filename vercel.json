{
	"version": 2,
	"public": false,
	"builds": [{ "src": "main.py", "use": "@vercel/python" }],
	"routes": [
		{ "src": "/", "dest": "main.py" },
		{ "src": "/docs", "dest": "main.py" },
		{ "src": "/openapi.json", "dest": "main.py" },
		{ "src": "/words", "dest": "app.py" },
		{ "src": "/anagrams", "dest": "app.py" },
		{ "src": "/words/(.*)", "dest": "app.py" },
		{ "src": "/anagrams/(.*)", "dest": "app.py" }
	]
}